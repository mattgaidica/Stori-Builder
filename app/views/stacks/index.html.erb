<p id="notice"><%= notice %></p>

<h1>Stacks</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Pref annotations showsource</th>
      <th>Pref settings importpath</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stacks.each do |stack| %>
      <tr>
        <td><%= stack.name %></td>
        <td><%= stack.pref_annotations_showsource %></td>
        <td><%= stack.pref_settings_importpath %></td>
        <td><%= link_to 'Show', stack %></td>
        <td><%= link_to 'Edit', edit_stack_path(stack) %></td>
        <td><%= link_to 'Destroy', stack, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stack', new_stack_path %>
